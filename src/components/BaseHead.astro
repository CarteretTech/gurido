---
import "../styles/global.css";
import { AstroSeo } from '@astrolib/seo';
import Favicons from "./global/Favicons.astro";
---
<AstroSeo
  title="Title"
  description="We create websites that convert visitors into customers Design and development subscriptions for everyone. Pause or cancel anytime."
  canonical="https://www.neuepower.com"
  openGraph={{
    url: "https://www.neuepower.com",
    title: "NeuePower",
    description: "We create websites that convert visitors into customers Design and development subscriptions for everyone. Pause or cancel anytime.",
    images: [
      {
        url: "https://www.neuepower.com/opengraph/social-og.jpg",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://www.neuepower.com/opengraph/twitter-og.jpg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://www.neuepower.com/opengraph/social-og.jpg" },
      { url: "https://www.neuepower.com/opengraph/twitter-og.jpg" },
    ],
    site_name: "NeuePower",
  }}
  twitter={{
    handle: "@Mike_Andreuzza",
    site: "https://neuepower.com",
    cardType: "summary_large_image",
  }}
/>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width" />
<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<meta name="author" content="Michael Andreuzza" />
<meta name="your keywords" content="Add ypour keywords here" />


<Favicons/>
<link rel="preconnect" href="https://rsms.me/" />
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
<script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>

<link
  href="https://api.fontshare.com/v2/css?f[]=jet-brains-mono@1,2&display=swap"
  rel="stylesheet"
/>