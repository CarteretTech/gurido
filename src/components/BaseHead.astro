---
import "../styles/global.css";
import { AstroSeo } from '@astrolib/seo';
import Favicons from "./global/Favicons.astro";
---
<AstroSeo
  title="GURIDO"
  description="We create websites that convert visitors into customers Design and development subscriptions for everyone. Pause or cancel anytime."
  canonical="https://www.yoursite.com"
  openGraph={{
    url: "https://www.yoursite.com",
    title: "monomod",
    description: "We create websites that convert visitors into customers Design and development subscriptions for everyone. Pause or cancel anytime.",
    images: [
      {
        url: "https://www.yoursite.com/opengraph/facebook.png",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://www.yoursite.com/opengraph/twitter.png",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://www.yoursite.com/opengraph/facebook.png" },
      { url: "https://www.yoursite.com/opengraph/twitter.png" },
    ],
    site_name: "monomod",
  }}
  twitter={{
    handle: "@Mike_Andreuzza",
    site: "https://monomod.studio",
    cardType: "summary_large_image",
  }}
/>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width" />
<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<meta name="author" content="Michael Andreuzza" />
<meta name="your keywords" content="Add ypour keywords here" />


<Favicons/>
<link href="https://api.fontshare.com/v2/css?f[]=comico@400&f[]=switzer@2,1&display=swap" rel="stylesheet">



<!-- Alpine.js Core -->
<script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"></script>



